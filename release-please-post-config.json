{
  "post-release-hooks": {
    "pkg/one": {
      "dispatch-pipelines": [
        {
          "filename": "on-tag-workflow.yaml"
        }
      ]
    },
    "pkg/.*": {
      "dispatch-pipelines": [
        {
          "filename": "post-release-workflow.yaml",
          "inputs": {
            "tag_name": "{{tag_name}}",
            "release_url": "{{release_url}}",
            "release_sha": "{{release_sha}}"
          }
        }
      ]
    }
  }
}
